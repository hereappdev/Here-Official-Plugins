{
    "name": "App Store Ranking",
    "version": "1.0.3",
    "description": "Show Apps rank list on App Store",
    "icon": "icon.png",
    "identifier": "app.here.appstoreranking",
    "createdBy": "Here",
    "website": "https://here.app",
    "appcast": "https://raw.githubusercontent.com/hereappdev/Here-Official-Plugins/master/source/App-Store-Ranking/appcast.xml",
    "script": "index.js",
    "defaultCategory": "news",
    "displayOn": ["mini", "menuBar"],
    "preferences": [
        {
            "title": "Country Code",
            "type": "text",
            "key": "countryCode",
            "default": "US"
        }
    ]
}
