{
    "name": "Stocks",
    "version": "1.3.4",
    "configURL": "https://raw.githubusercontent.com/hereappdev/Here-Official-Plugins/master/source/Stocks/config.json",
    "downloadURL": "https://raw.githubusercontent.com/hereappdev/Here-Official-Plugins/master/downloads/Stocks.hereplugin",
    "description": "Show stocks everywhere.",
    "icon": "icon.png",
    "identifier": "app.here.stocks",
    "createdBy": "Here",
    "website": "https://here.app",
    "script": "index.js",
    "defaultCategory": "fav",
    "displayOn": ["mini", "menuBar", "dock"],
    "preferences": [
        {
            "title": "Stock Name",
            "type": "text",
            "key": "stockName",
            "default": "Apple Inc."
        },
        {
            "title": "Stock Code",
            "type": "text",
            "key": "stockCode",
            "default": "gb_AAPL"
        }
    ]
}
