{
    "name": "Weather",
    "version": "1.0.5",
    "configURL": "https://raw.githubusercontent.com/hereappdev/Here-Official-Plugins/master/source/Weather/config.json",
    "downloadURL": "https://raw.githubusercontent.com/hereappdev/Here-Official-Plugins/master/downloads/Weather.hereplugin",
    "description": "Show world weather",
    "icon": "icon.png",
    "identifier": "app.here.weather",
    "createdBy": "Here",
    "website": "https://here.app",
    "script": "index.js",
    "defaultCategory": "fav",
    "displayOn": ["mini", "menuBar", "dock"],
    "preferences": [
        {
            "title": "Location",
            "type": "text",
            "key": "location",
            "default": "newyork"
        },
        {
            "title": "Degree Units",
            "type": "popup",
            "key": "degreeUnits",
            "default": 0,
            "options": ["℉ - Fahrenheit", "℃ - Celsius"]
        }
    ]
}
