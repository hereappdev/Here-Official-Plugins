{
    "name": "Github Trending",
    "version": "1.0.5",
    "configURL": "https://raw.githubusercontent.com/hereappdev/Here-Official-Plugins/master/source/Github-Trending/config.json",
    "downloadURL": "https://raw.githubusercontent.com/hereappdev/Here-Official-Plugins/master/downloads/Github-Trending.hereplugin",
    "description": "Show what the GitHub is most excited about today",
    "icon": "icon.png",
    "identifier": "app.here.githubtrending",
    "createdBy": "Here",
    "website": "https://here.app",
    "script": "index.js",
    "defaultCategory": "news",
    "displayOn": ["mini", "menuBar"],
    "preferences": [
        {
            "title": "Code Language",
            "type": "text",
            "key": "codeLanguage",
            "default": ""
        },
        {
            "title": "Spoken Language Code",
            "type": "text",
            "key": "spokenLanguageCode",
            "default": "en"
        }
    ]
}
